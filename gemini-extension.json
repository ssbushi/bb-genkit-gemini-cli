{
  "name": "genkit",
  "version": "1.2.0",
  "mcpServers": {
    "genkit": {
      "command": "npx",
      "args": [
        "-y",
        "genkit-cli@1.21.0",
        "mcp",
        "--",
        "--no-update-notification",
        "--non-interactive"
      ],
      "cwd": ".",
      "timeout": 30000,
      "trust": false,
      "excludeTools": [
        "run_shell_command(genkit start)",
        "run_shell_command(npx genkit start)"
      ]
    }
  },
  "contextFileName": "GENKIT.md"
}
